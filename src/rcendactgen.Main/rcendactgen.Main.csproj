      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\rcendactgen.Business\rcendactgen.Business.csproj" />
    <ProjectReference Include="..\rcendactgen.Common\rcendactgen.Common.csproj" />
  </ItemGroup>

</Project>
